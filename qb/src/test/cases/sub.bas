SUB MYSUB (X() AS INTEGER)
  Print X(20)
  PRINT X%(19)
END SUB

SUB MUL (R#, X#, Y#)
  R# = X# * Y#
END SUB

SUB MULADD (R#, X#, Y#)
  MUL a#, X#, Y#
  MUL b#, X#, Y#
  R# = a# + b#
END SUB

SUB PARAMS (x%, y AS INTEGER)
  PRINT x%;y%;y
END SUB

SUB PRINTIT (x$, y$)
  PRINT x$;y$
END SUB

