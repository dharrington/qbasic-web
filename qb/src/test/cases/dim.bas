CONST THREE = 3
CONST FOUR = 4
DIM XX(THREE,FOUR) AS INTEGER, YY AS STRING
FOR X = 1 TO 3
FOR Y = 1 TO 4
  XX(X,Y)=X*Y
NEXT Y
NEXT X

FOR X = 1 TO 3
  FOR Y = 1 TO 4
    PRINT XX(X,Y);
  NEXT Y
  PRINT
NEXT X
YY = "HI"
PRINT YY
FOR X = 1 TO 3
  DIM INLOOP AS INTEGER
  PRINT INLOOP;
  INLOOP = INLOOP + 1
NEXT X
