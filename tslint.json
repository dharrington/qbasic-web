{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "member-access": false,
        "max-classes-per-file": false,
        "curly": false,
        "no-bitwise": false,
        "no-empty": false,
        "no-console": false,
        "max-line-length": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}